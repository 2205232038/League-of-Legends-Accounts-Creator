{
  "name": "acc-creator-react",
  "version": "1.4.10",
  "description": "League of Legends accounts creator",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/renderer.webpack.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "cross-env NODE_ENV=production webpack --config webpack/electron.webpack.js --mode production",
    "build:react": "cross-env NODE_ENV=production webpack --config webpack/renderer.webpack.js --mode production",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder"
  },
  "keywords": [
    "lol",
    "riot",
    "creators"
  ],
  "author": "Dmitriy Frolov <omgoole@ya.ru>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lociero/League-of-Legends-Accounts-Creator.git"
  },
  "license": "MIT",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "bootswatch": "^4.6.0",
    "clean-webpack-plugin": "^3.0.0",
    "compare-versions": "^3.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "custom-electron-titlebar": "^3.2.7",
    "electron": "^10.4.7",
    "electron-builder": "^22.10.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "express": "^4.17.1",
    "generate-password": "^1.6.0",
    "html-webpack-plugin": "^5.3.1",
    "https-proxy-agent": "^5.0.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.4.2",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-hooks-global-state": "^1.0.1",
    "react-tooltip": "^4.2.21",
    "socks-proxy-agent": "^5.0.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.3.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {},
  "build": {
    "appId": "acc.creator",
    "win": {
      "target": "portable",
      "icon": "renderer/imgs/icon.ico",
      "files": [
        "package.json",
        "dist/**"
      ]
    },
    "directories": {
      "output": "packages"
    }
  }
}
